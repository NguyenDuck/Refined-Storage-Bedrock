{
    "format_version": "1.20.10",
    "minecraft:block": {
        "description": {
            "identifier": "refinedstorage:exporter",
            "menu_category": {
                "category": "items",
                "group": "itemGroup.name.refinedstorage.exporter"
            },
            "properties": {
                "refinedstorage:north": [
                    false,
                    true
                ],
                "refinedstorage:south": [
                    false,
                    true
                ],
                "refinedstorage:east": [
                    false,
                    true
                ],
                "refinedstorage:west": [
                    false,
                    true
                ],
                "refinedstorage:up": [
                    false,
                    true
                ],
                "refinedstorage:down": [
                    false,
                    true
                ],
                "refinedstorage:module_direction": [
                    "north",
                    "south",
                    "east",
                    "west",
                    "up",
                    "down"
                ],
                "refinedstorage:placed": [
                    false,
                    true
                ]
            },
            "traits": {
                "minecraft:placement_position": {
                    "enabled_states": [
                        "minecraft:block_face"
                    ]
                }
            }
        },
        "components": {
            "tag:refinedstorage:exporter": {},
            "tag:refinedstorage:component": {},
            "minecraft:destructible_by_mining": {
                "seconds_to_destroy": 0.35
            },
            "minecraft:destructible_by_explosion": {
                "explosion_resistance": 0.35
            },
            "minecraft:geometry": {
                "identifier": "geometry.exporter",
                "bone_visibility": {
                    "module_north": "q.block_property('refinedstorage:module_direction') == 'south'",
                    "module_south": "q.block_property('refinedstorage:module_direction') == 'north' || !q.block_property('refinedstorage:placed')",
                    "module_east": "q.block_property('refinedstorage:module_direction') == 'west'",
                    "module_west": "q.block_property('refinedstorage:module_direction') == 'east'",
                    "module_up": "q.block_property('refinedstorage:module_direction') == 'down'",
                    "module_down": "q.block_property('refinedstorage:module_direction') == 'up'",
                    "north": "q.block_property('refinedstorage:module_direction') != 'south' && q.block_property('refinedstorage:north')",
                    "south": "q.block_property('refinedstorage:module_direction') != 'north' && q.block_property('refinedstorage:south')",
                    "east": "q.block_property('refinedstorage:module_direction') != 'west' && q.block_property('refinedstorage:east') || !q.block_property('refinedstorage:placed')",
                    "west": "q.block_property('refinedstorage:module_direction') != 'east' && q.block_property('refinedstorage:west') || !q.block_property('refinedstorage:placed')",
                    "up": "q.block_property('refinedstorage:module_direction') != 'down' && q.block_property('refinedstorage:up')",
                    "down": "q.block_property('refinedstorage:module_direction') != 'up' && q.block_property('refinedstorage:down')"
                }
            },
            "minecraft:material_instances": {
                "*": {
                    "texture": "importer",
                    "render_method": "blend",
                    "ambient_occlusion": false
                }
            },
            "minecraft:queued_ticking": {
                "interval_range": [
                    0,
                    0
                ],
                "looping": true,
                "on_tick": {
                    "event": "refinedstorage:component_connect"
                }
            },
            "minecraft:on_placed": {
                "event": "refinedstorage:placed"
            },
            "minecraft:on_player_placing": {
                "event": "refinedstorage:player_placed"
            }
        },
        "events": {
            "refinedstorage:component_connect": {
                "set_block_property": {
                    "refinedstorage:north": "q.block_neighbor_has_any_tag(0, 0, -1, 'refinedstorage:component')",
                    "refinedstorage:south": "q.block_neighbor_has_any_tag(0, 0, 1, 'refinedstorage:component')",
                    "refinedstorage:east": "q.block_neighbor_has_any_tag(1, 0, 0, 'refinedstorage:component')",
                    "refinedstorage:west": "q.block_neighbor_has_any_tag(-1, 0, 0, 'refinedstorage:component')",
                    "refinedstorage:up": "q.block_neighbor_has_any_tag(0, 1, 0, 'refinedstorage:component')",
                    "refinedstorage:down": "q.block_neighbor_has_any_tag(0, -1, 0, 'refinedstorage:component')"
                }
            },
            "refinedstorage:player_placed": {
                "set_block_property": {
                    "refinedstorage:module_direction": "q.block_property('minecraft:block_face')",
                    "refinedstorage:placed": true
                }
            },
            "refinedstorage:placed": {
                "set_block_property": {
                    "refinedstorage:module_direction": "q.block_property('minecraft:block_face')",
                    "refinedstorage:placed": true
                },
                "run_command": {
                    "command": "/execute positioned ~~~ unless entity @e[type=refinedstorage:exporter, c=1, r=0.5] run summon refinedstorage:exporter"
                }
            }
        }
    }
}