{
    "format_version": "1.20.10",
    "minecraft:block": {
        "description": {
            "identifier": "refinedstorage:importer",
            "menu_category": {
                "category": "items",
                "group": "itemGroup.name.refinedstorage.importer"
            },
            "properties": {
                "refinedstorage:north": [
                    false,
                    true
                ],
                "refinedstorage:south": [
                    false,
                    true
                ],
                "refinedstorage:east": [
                    false,
                    true
                ],
                "refinedstorage:west": [
                    false,
                    true
                ],
                "refinedstorage:up": [
                    false,
                    true
                ],
                "refinedstorage:down": [
                    false,
                    true
                ],
                "refinedstorage:module_north": [
                    false,
                    true
                ],
                "refinedstorage:module_south": [
                    false,
                    true
                ],
                "refinedstorage:module_east": [
                    false,
                    true
                ],
                "refinedstorage:module_west": [
                    false,
                    true
                ],
                "refinedstorage:module_up": [
                    false,
                    true
                ],
                "refinedstorage:module_down": [
                    false,
                    true
                ],
                "refinedstorage:placed": [
                    false,
                    true
                ]
            },
            "traits": {
                "minecraft:placement_position": {
                    "enabled_states": [
                        "minecraft:block_face"
                    ]
                }
            }
        },
        "components": {
            "tag:refinedstorage:importer": {},
            "tag:refinedstorage:component": {},
            "minecraft:destructible_by_mining": {
                "seconds_to_destroy": 0.35
            },
            "minecraft:destructible_by_explosion": {
                "explosion_resistance": 0.35
            },
            "minecraft:geometry": {
                "identifier": "geometry.importer",
                "bone_visibility": {
                    "module_north": "q.block_property('refinedstorage:module_north')",
                    "module_south": "q.block_property('refinedstorage:module_south') || !q.block_property('refinedstorage:placed')",
                    "module_east": "q.block_property('refinedstorage:module_east')",
                    "module_west": "q.block_property('refinedstorage:module_west')",
                    "module_up": "q.block_property('refinedstorage:module_up')",
                    "module_down": "q.block_property('refinedstorage:module_down')",
                    "north": "!q.block_property('refinedstorage:module_north') && q.block_property('refinedstorage:north')",
                    "south": "!q.block_property('refinedstorage:module_south') && q.block_property('refinedstorage:south')",
                    "east": "!q.block_property('refinedstorage:module_east') && q.block_property('refinedstorage:east') || !q.block_property('refinedstorage:placed')",
                    "west": "!q.block_property('refinedstorage:module_west') && q.block_property('refinedstorage:west') || !q.block_property('refinedstorage:placed')",
                    "up": "!q.block_property('refinedstorage:module_up') && q.block_property('refinedstorage:up')",
                    "down": "!q.block_property('refinedstorage:module_down') && q.block_property('refinedstorage:down')"
                }
            },
            "minecraft:material_instances": {
                "*": {
                    "texture": "importer",
                    "render_method": "blend",
                    "ambient_occlusion": false
                }
            },
            "minecraft:queued_ticking": {
                "interval_range": [
                    0,
                    0
                ],
                "looping": true,
                "on_tick": {
                    "event": "refinedstorage:component_connect"
                }
            },
            "minecraft:on_player_placing": {
                "event": "refinedstorage:component_rotate"
            }
        },
        "events": {
            "refinedstorage:component_connect": {
                "set_block_property": {
                    "refinedstorage:north": "q.block_neighbor_has_any_tag(0, 0, -1, 'refinedstorage:component')",
                    "refinedstorage:south": "q.block_neighbor_has_any_tag(0, 0, 1, 'refinedstorage:component')",
                    "refinedstorage:east": "q.block_neighbor_has_any_tag(1, 0, 0, 'refinedstorage:component')",
                    "refinedstorage:west": "q.block_neighbor_has_any_tag(-1, 0, 0, 'refinedstorage:component')",
                    "refinedstorage:up": "q.block_neighbor_has_any_tag(0, 1, 0, 'refinedstorage:component')",
                    "refinedstorage:down": "q.block_neighbor_has_any_tag(0, -1, 0, 'refinedstorage:component')"
                }
            },
            "refinedstorage:component_rotate": {
                "set_block_property": {
                    "refinedstorage:module_north": "q.block_property('minecraft:block_face') == 'south'",
                    "refinedstorage:module_south": "q.block_property('minecraft:block_face') == 'north'",
                    "refinedstorage:module_east": "q.block_property('minecraft:block_face') == 'west'",
                    "refinedstorage:module_west": "q.block_property('minecraft:block_face') == 'east'",
                    "refinedstorage:module_up": "q.block_property('minecraft:block_face') == 'down'",
                    "refinedstorage:module_down": "q.block_property('minecraft:block_face') == 'up'",
                    "refinedstorage:placed": true
                }
            }
        }
    }
}